{
    "name": "amf/webservices-client-bundle",
    "description": "A bundle for communication with ReST and Soap web services",
    "type": "symfony-bundle",
    "keywords": ["WebServices", "Client", "ReST", "Soap"],
    "homepage": "https://github.com/fattouchsquall",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Amine Fattouch",
            "email": "amine.fattouch@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5.9",
        "symfony/framework-bundle": "^2.7",
        "symfony/serializer": "^2.7"
    },
    "require-dev": {
       "phake/phake": "@stable",
       "phpunit/phpunit": "~4.8|~5.0"
    },
    "autoload": {
        "psr-4" : { "AMF\\WebServicesClientBundle\\" : "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
